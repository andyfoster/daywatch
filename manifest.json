{
  "name": "DayWatch: New Tab Countdown Tracker",
  "short_name": "DayWatch",
  "version": "0.0.1",
  "description": "Keep track of days until an event on your new tab page.",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "192": "icons/icon192.png",
    "512": "icons/icon512.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
